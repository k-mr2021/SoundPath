<div class= "container">
  <div class= "row justify-content-center">
    <div class= "col-lg-auto col-sm-auto col-auto">
      <%- if controller_name != 'sessions' %>
        <%= link_to "Log in", new_session_path(resource_name), class: "btn btn-primary" %><b>はこちらから</b>
      <% end %>
    </div>
  </div>
  <div class= "row justify-content-center">
    <div class= "col-lg-auto col-sm-auto col-auto">
      <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
        <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-primary" %><b>はこちらから</b>
      <% end %>
    </div>
  </div>
</div>






